  <main class="container my-4">
    <div class="row g-4">
      <div class="col-md-8">
        <div id="karuzela" class="carousel slide cien mb-3">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img src="https://fastly.picsum.photos/id/203/300/100.jpg?hmac=C5aOVTkrwPjlJrYJV05IWjkxgurTQrf3m7vcdn9Q8vc" class="d-block w-100">
